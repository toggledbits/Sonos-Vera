<?xml version="1.0"?>
<!-- S_SonosSystem1.xml -->
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Devices</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Zones</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_ipaddr</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_enable</name>
			<dataType>ui1</dataType>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>PauseAll</name>
		</action>
		<action>
			<name>SavePlaybackContext</name>
			<argumentList>
				<argument>
					<name>GroupDevices</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Devices</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Zones</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>RestorePlaybackContext</name>
		</action>
		<action>
			<name>SetupTTS</name>
			<argumentList/>
		</action>
		<action>
			<name>ResetTTS</name>
			<argumentList />
		</action>
		<action>
			<name>IncludeIP</name>
			<argumentList>
				<argument>
					<name>IPAddress</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_ipaddr</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetDebugLogs</name>
			<argumentList>
				<argument>
					<name>enable</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_enable</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>InstallDiscoveryPatch</name>
		</action>
		<action>
			<name>UninstallDiscoveryPatch</name>
		</action>
		<action>
			<name>StartSonosDiscovery</name>
			<argumentList/>
		</action>
	</actionList>
</scpd>