<?xml version="1.0"?>
<!-- S_Sonos1.xml -->
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_URI</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Volume</name>
			<dataType>ui2</dataType>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>100</maximum>
				<step>1</step>
			</allowedValueRange>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_EnqueueMode</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>ENQUEUE</allowedValue>
				<allowedValue>ENQUEUE_AND_PLAY</allowedValue>
				<allowedValue>ENQUEUE_AT_FIRST</allowedValue>
				<allowedValue>ENQUEUE_AT_FIRST_AND_PLAY</allowedValue>
				<allowedValue>ENQUEUE_AT_NEXT_PLAY</allowedValue>
				<allowedValue>REPLACE_QUEUE</allowedValue>
				<allowedValue>REPLACE_QUEUE_AND_PLAY</allowedValue>
			</allowedValueList>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_SameVolume</name>
			<dataType>boolean</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Duration</name>
			<dataType>ui4</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Devices</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Zones</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Text</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Language</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Zone</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Name</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_IP</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_boolean</name>
			<dataType>boolean</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_LastChange</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_ZoneGroupState</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_ContainerUpdateIDs</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Engine</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>GOOGLE</allowedValue>
				<allowedValue>OSX_TTS_SERVER</allowedValue>
				<allowedValue>MICROSOFT</allowedValue>
				<allowedValue>MARY</allowedValue>
				<allowedValue>RV</allowedValue>
			</allowedValueList>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>SonosOnline</name>
			<shortCode>online</shortCode>
			<dataType>boolean</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_enable</name>
			<dataType>boolean</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_clientId</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_clientSecret</name>
			<dataType>string</dataType>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>SetURIToPlay</name>
			<argumentList>
				<argument>
					<name>URIToPlay</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_URI</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>PlayURI</name>
			<argumentList>
				<argument>
					<name>URIToPlay</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_URI</relatedStateVariable>
				</argument>
				<argument>
					<name>Volume</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Volume</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>EnqueueURI</name>
			<argumentList>
				<argument>
					<name>URIToEnqueue</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_URI</relatedStateVariable>
				</argument>
				<argument>
					<name>EnqueueMode</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_EnqueueMode</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>Alert</name>
			<argumentList>
				<argument>
					<name>URI</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_URI</relatedStateVariable>
				</argument>
				<argument>
					<name>Volume</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Volume</relatedStateVariable>
				</argument>
				<argument>
					<name>SameVolumeForAll</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_SameVolume</relatedStateVariable>
				</argument>
				<argument>
					<name>Duration</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Duration</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupDevices</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Devices</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Zones</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>Say</name>
			<argumentList>
				<argument>
					<name>Text</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Text</relatedStateVariable>
				</argument>
				<argument>
					<name>Language</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Language</relatedStateVariable>
				</argument>
				<argument>
					<name>Engine</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Engine</relatedStateVariable>
				</argument>
				<argument>
					<name>Volume</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Volume</relatedStateVariable>
				</argument>
				<argument>
					<name>SameVolumeForAll</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_SameVolume</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupDevices</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Devices</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Zones</relatedStateVariable>
				</argument>
				<argument>
					<name>Chime</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_boolean</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>PauseAll</name>
		</action>
		<action>
			<name>JoinGroup</name>
			<argumentList>
				<argument>
					<name>Zone</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Zone</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>LeaveGroup</name>
		</action>
		<action>
			<name>UpdateGroupMembers</name>
			<argumentList>
				<argument>
					<name>Zones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Zones</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SelectSonosDevice</name>
			<argumentList>
				<argument>
					<name>URL</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_URI</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetCheckStateRate</name>
			<argumentList>
				<argument>
					<name>rate</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Duration</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetReadQueueContent</name>
			<argumentList>
				<argument>
					<name>enable</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_enable</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>